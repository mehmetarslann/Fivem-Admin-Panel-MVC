@model IEnumerable<AdminPanel.Models.Entity.launcher_uyeler>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Launcher Üyeleri</h2>

<table class="table">
    <tr>
        <th>
            @Html.Label("Ad")
        </th>
        <th>
            @Html.Label("Soyad")
        </th>
        <th>
            @Html.Label("Kullanıcı Adı")
        </th>
        <th>
            @Html.Label("Parola")
        </th>
        <th>
            @Html.Label("Son Giriş Tarihi")
        </th>
        <th>
            @Html.Label("Steam ID")
        </th>
        <th>
            @Html.Label("Discord ID")
        </th>
        <th>
            @Html.Label("Hile Tespit Durumu")
        </th>
        <th>
            @Html.Label("Online Durum")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.uye_ad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.uye_soyad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.uye_kadi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.uye_parola)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.uye_gtarih)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.steam_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.discord_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hile_tespiti)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.login_durum)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "Edit", new { id=item.uye_id }) |
            @Html.ActionLink("Sil", "Delete", new { id=item.uye_id })
        </td>
    </tr>
}

</table>
